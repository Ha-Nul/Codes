@article {1307,
	title = {Quantum Mechanics as a Principle Theory},
	year = {1999},
	month = {1999/10/22},
	abstract = {  I show how quantum mechanics, like the theory of relativity, can be
understood as a {\textquoteright}principle theory{\textquoteright} in Einstein{\textquoteright}s sense, and I use this notion
to explore the approach to the problem of interpretation developed in my book
