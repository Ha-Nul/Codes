	url = {http://www.osa-opn.org/abstract.cfm?URI=opn-24-12-48},
	author = {Firstenberg, O and Lukin, M D and Peyronel, T and Liang, Q -Y and Vuletic, V and Alexey V. Gorshkov and Hofferberth, S and Pohl, T}
