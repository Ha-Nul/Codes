	doi = {https://doi.org/10.1016/j.nima.2018.06.076},
	url = {https://arxiv.org/abs/1805.11306},
	author = {K. Pushkin and C. Akerlof and D. Anbajagane and J. Armstrong and M. Arthurs and Jacob Bringewatt and T. Edberg and C. Hall and M. Lei and R. Raymond and M. Reh and D. Saini and A. Sander and J. Schaefer and D. Seymour and N. Swanson and Y. Wang and W. Lorenzon}
